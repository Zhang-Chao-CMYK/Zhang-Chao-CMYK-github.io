function cutfilename(_fname,_index)
{
    var _is_student = false;
    if(window.gconfig && gconfig.role && gconfig.role=='student')
        _is_student = true;

    if(!_is_student) {
        var _ext = _fname.substr(_fname.lastIndexOf('.'));
        var _fn = _fname.substr(0, _fname.lastIndexOf('.'));
        if (!_ext || _ext.length < 2 || _ext.length > 5) {
            _ext = '';
            _fn = _fname;
        }

        _fn = _fn.replace(/[^a-zA-Z\u4e00-\u9fa5\d]/g, '');
        if (_ext != '')
            _fn = _fn + _ext;

        return _fn;
    }

    var _fnm = '附件';
    if(typeof(_index)!='undefined')
        _fnm+=_index;
    _fnm+=_fname.substr(_fname.lastIndexOf('.'));
    return _fnm;
}




var canvas = document.createElement("canvas");
var ctx = canvas.getContext('2d');
//    瓦片canvas
var tCanvas = document.createElement("canvas");
var tctx = tCanvas.getContext("2d");

function compress(img) {
	var initSize = img.src.length;
	var width = img.width;
	var height = img.height;

	var ratio;
    max_px = 1000000;
    if(typeof(bjmf_img_max_px)!='undefined' )//可以全局设置一个参数来重新指定尺寸
        max_px = bjmf_img_max_px;
    compress_jpg_quality = 0.5;//图片质量0-1
    if(typeof(bjmf_compress_jpg_quality)!='undefined' )
        compress_jpg_quality = bjmf_compress_jpg_quality;

	if ((ratio = width * height / max_px) > 1) {
	  ratio = Math.sqrt(ratio);
	  width /= ratio;
	  height /= ratio;
	} else {
	  ratio = 1;
	}
	canvas.width = width;
	canvas.height = height;

	ctx.fillStyle = "#fff";
	ctx.fillRect(0, 0, canvas.width, canvas.height);
	//如果图片像素大于100万则使用瓦片绘制
	var count;
	ctx.drawImage(img, 0, 0, width, height);
	//进行最小压缩
	var ndata = canvas.toDataURL('image/jpeg',compress_jpg_quality);//0.1
	tCanvas.width = tCanvas.height = canvas.width = canvas.height = 0;
	return ndata;
}
/**
* 获取blob对象的兼容性写法
* @param buffer
* @param format
* @returns {*}
*/
function getBlob(buffer, format) {
	try {
	  return new Blob(buffer, {type: format});
	} catch (e) {
	  var bb = new (window.BlobBuilder || window.WebKitBlobBuilder || window.MSBlobBuilder);
	  buffer.forEach(function(buf) {
	    bb.append(buf);
	  });
	  return bb.getBlob(format);
	}
}

function random_string(len) {
    len = len || 32;
    var chars = 'ABCDEFGHJKMNPQRSTWXYZabcdefhijkmnprstwxyz2345678';
    var maxPos = chars.length;
    var pwd = '';
    for (i = 0; i < len; i++) {
        pwd += chars.charAt(Math.floor(Math.random() * maxPos));
    }
    return pwd;
}

function get_file_suffix(filename) {
    var pos = filename.lastIndexOf('.')
    var suffix = ''
    if (pos != -1) {
        suffix = filename.substring(pos)
    }
    return suffix;
}

function get_file_object_name(filename) {
    var suffix = get_file_suffix(filename)
    return (Date.parse(new Date()) / 1000) + random_string(4) + suffix
}

function show_alt_on_mobile() {
    if (location.hostname === 'exam.hans') {
        return true;
    }
    var ua = navigator.userAgent;
    if (!ua) {
        return false;
    }
    if (ua.indexOf('MicroMessenger') < 0) {
        return false;
    }
    ua = ua.toLowerCase();
    if (ua.indexOf('android') > 0) {
        return true;
    }
    return ua.indexOf('wechatdevtools') > 0;
}

function use_alt_upload(evt) {
    evt.preventDefault();
    evt.stopPropagation();
    var util = $(evt.target).parents('.jFiler').find('input[type=file]').data('util');
    if (!util) {
        return;
    }
    loadWeChat(true, [
        'chooseImage', 'getLocalImgData'
    ], function () {
        wx.chooseImage({
            count: 1,
            sizeType: ['compressed'],
            success: function (res) {
                var localIds = res.localIds;
                for (var i = 0; i < localIds.length; i++) {
                    wx.getLocalImgData({
                        localId: localIds[i],
                        success: function (res) {
                            var _lcdata = res.localData;
                            if(_lcdata.substr(0,5)!='data:')
                                _lcdata = 'data:image/jpg;base64,'+_lcdata;

                            var img = new Image();
                            img.onload = function () {
                                var data = compress(img);
                                var file = data_url_to_file(data);
                                util._onChange(null, [file]);
                            };
                            img.src = _lcdata;
                        }
                    });
                }
            }
        });
    });
}

function data_url_to_file (dataUrl) {
    var arr = dataUrl.split(',');
    var mime = 'image/jpg';
    var base64 = dataUrl;
    if (arr.length > 1) {
        mime = arr[0].match(/:(.*?);/)[1];
        base64 = arr[1];
    }
    var bstr = atob(base64);
    var n = bstr.length;
    var u8arr = new Uint8Array(n);
    while(n--){
        u8arr[n] = bstr.charCodeAt(n);
    }
    var ext = mime.split('/')[1];
    var filename = 'wx_' + (+new Date()) + '.' + ext;
    return new File([u8arr], filename, { type: mime });
}

function show_upload_on_pc_tip() {
    try {
        var path = location.pathname;
        if (!path || !path.startsWith('/teacher')) {
            return false;
        }
        var ua = navigator.userAgent;
        if (!ua) {
            return false;
        }
        ua = ua.toLowerCase();
        if (ua.indexOf('like mac os x') > 0) {
            return true;
        }
        if (ua.indexOf("android") > 0) {
            var reg = /android [\d._]+/gi;
            var v_info = ua.match(reg);
            var version = (v_info + "").replace(/[^0-9|_.]/ig, "").replace(/_/ig, ".");
            version = parseInt(version.split('.')[0]);
            if (version == 10) {
                return true;
            }
        }
    } catch (e) {
        console.log(e);
    }
    return false;
}

var defaultChangeInput2 = '<div class="jFiler-input-dragDrop"><div class="jFiler-input-inner"><div class="jFiler-input-icon"><i class="icon-cloud-upload"></i></div><div class="jFiler-input-text"><h3>拖拽文件到此处</h3> <span style="display:inline-block; margin: 15px 0">或</span></div><a class="jFiler-input-choose-btn blue-light">点此选择要上传的文件</a></div></div>';
