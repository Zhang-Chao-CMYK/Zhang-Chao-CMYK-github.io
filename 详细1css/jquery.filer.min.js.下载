    defaultChangeInput2 += '<style>.jFiler-input-inner:after {content: \'如果不会用手机上传，可以用电脑版微信或浏览器上传\';display: block;margin-top: 12px;font-size: 12px;}</style>';
}
if (show_alt_on_mobile()) {
    defaultChangeInput2 += '<div style="margin-top:10px;margin-bottom:10px;font-size:12px"><a href="javascript:" onclick="use_alt_upload(event)" style="color:#aaa">选择了文件，上传没反应？</a></div>';
}

