		$.each($(obj)[0].jFiler.files_list,function(i,v){
			_data.push({
				name:cutfilename(v.file.name,i+1),
				file:v.file.file,
				size:parseInt(v.file.size/1024),
				type:v.file.type,
			});
		});
		if(typeof(kkbupfileschged) === "function")
			kkbupfileschged(_data,$(obj)[0].id.replace('bjmfuploader_',''));
    },
    ossRole: '',
    ossPath: '',
    ossKey: null,
    beforeSelect: function (files) {
        return true;
    },
    prepareInput: function (input, options) {
        if (options && options.capture) {
            input.attr('capture', options.capture);
            
            if (!options.accept) {
                options.accept = 'image/*';
            }
            input.attr('accept', options.accept);

            if (!in_weixin() || !in_mobile())
            {
                input.attr('disabled', 'true');
            }
        }
        return input;
    },
    prepareKey: function (filer, formData, fileName, callback) {
        var host = location.hostname;
        var local = host.startsWith('localhost') || host.endsWith('.local') || host.endsWith('.hans');
        if (local) {
            filer.uploadFile.url = filer_default_opts.uploadFile.url;
            return callback('local');
        }

        function callback2(items) {
            filer.uploadFile.url = items.host;

            formData.append('key', items.dir + get_file_object_name(fileName));
            formData.append('policy', items.policy);
            formData.append('OSSAccessKeyId', items.accessid);
            formData.append('success_action_status', '200');
            formData.append('callback', items.callback);
            formData.append('signature', items.signature);

            callback(items);
        }

        if (filer_default_opts.ossKey) {
            var now = Date.parse(new Date()) / 1000;
            if (filer_default_opts.ossKey.expire > now + 3) {
                return callback2(filer_default_opts.ossKey);
            }
        }
        $.get('/' + filer_default_opts.ossRole + '/oss-upload-key', function (resp) {
            filer_default_opts.ossKey = resp;
            callback2(resp);
        }).fail(function () {
            alert('获取上传信息失败，请刷新页面后重试！');
        });
    }
};
