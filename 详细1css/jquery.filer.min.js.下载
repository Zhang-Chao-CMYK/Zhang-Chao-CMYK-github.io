/*
这里装了FormData，要把图片类文件压缩一下，手机拍照动则5M 的大小，根本吃不消
*/
_upload: function() {
    var i = u._itFc.html//li.jFiler-item.jFiler-no-thumbnail对象   l是上传的input框的对象
      , t = new FormData;  //u._itFc.file.name上传的原始文件名
      var _f = u._itFc.file;
      var _fname = _f.name ? _f.name : !1;//原始文件名

    if (!_fname) {
        return;
    }
    var exts = _fname.split('.');
    var ext = exts[exts.length - 1].toLowerCase();
    if (filer_default_opts.extensions.indexOf(ext) < 0) {
        $(i).remove();
        ((weui && weui.alert) || alert)('不合法的文件格式！');
        return;
    }

    t.append('name', _fname);

      //要对_f动手进行压缩
  if (/\/(?:jpeg|png|gif)/i.test(_f.type))//图片进行压缩后再上传
  {
      var reader = new FileReader();
      reader.onload = function() {
        var result = this.result;
        var img = new Image();
        img.src = result;
        if (img.complete) {
          callback();
        } else {
          img.onload = callback;
        }
        function callback() {
          var data = compress(img);
          img = null;

          try {
              var text = window.atob(data.split(",")[1]);
          }catch(e){
              alert('上传失败，无法识别图片(图片格式有问题？)');
              return;
          }


		    var buffer = new Uint8Array(text.length);
		    var pecent = 0, loop = null;
		    for (var ___i = 0; ___i < text.length; ___i++) {
		      buffer[___i] = text.charCodeAt(___i);
		    }
		    var blob = getBlob([buffer], _f.type);

            filer_default_opts.prepareKey(f, t, _fname, function (arg) {
                var fieldName = arg === 'local' ? l.attr('name') : 'file';
                if (t.append(fieldName, blob, _fname),
                null != f.uploadFile.data && e.isPlainObject(f.uploadFile.data))
                    for (var n in f.uploadFile.data)//这个看看配置里这个data，每次上传formdata还要不要再附带一下其他参数
                        t.append(n, f.uploadFile.data[n]);

                u._ajax.send(i, t, u._itFc);
            });
        }
      };
      reader.readAsDataURL(_f)
  }
  else
  {
      filer_default_opts.prepareKey(f, t, _fname, function (arg) {
          var fieldName = arg === 'local' ? l.attr('name') : 'file';
          if (t.append(fieldName, _f, _fname),
          null != f.uploadFile.data && e.isPlainObject(f.uploadFile.data))
              for (var n in f.uploadFile.data)//这个看看配置里这个data，每次上传formdata还要不要再附带一下其他参数
                  t.append(n, f.uploadFile.data[n]);

          u._ajax.send(i, t, u._itFc)
      });
  }
},
